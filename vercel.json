{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/css/(.*)",
      "dest": "src/public/css/$1"
    },
    {
      "src": "/public/js/(.*)",
      "dest": "src/public/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}
