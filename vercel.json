{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/((?!.*\\.\\w+$).*)",
      "dest": "src/index.js"
    },
    {
      "src": "/(.+\\.[a-z]+)$",
      "dest": "src/public/$1"
    }
  ]
  
}
