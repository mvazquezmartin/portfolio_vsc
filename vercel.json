{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [    
    {
      "src": "/css/(.*)",
      "dest": "src/public/css/$1"
    },
    {
      "src": "/index.js",
      "dest": "src/public/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}
